{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ng-omni-search": {
      "projectType": "library",
      "root": "projects/ng-omni-search",
      "sourceRoot": "projects/ng-omni-search/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ng-omni-search/ng-package.json"
          },
          "assets": [
            "projects/ng-omni-search/src/lib/assets"
          ],
          "configurations": {
            "production": {
              "tsConfig": "projects/ng-omni-search/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ng-omni-search/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/ng-omni-search/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "demo-test": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/demo-test",
      "sourceRoot": "projects/demo-test/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/demo-test",
            "index": "projects/demo-test/src/index.html",
            "main": "projects/demo-test/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/demo-test/tsconfig.app.json",
            "assets": [
              "projects/demo-test/src/favicon.ico",
              "projects/demo-test/src/assets"
            ],
            "styles": [
              "projects/demo-test/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "demo-test:build:production"
            },
            "development": {
              "browserTarget": "demo-test:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "demo-test:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/demo-test/tsconfig.spec.json",
            "assets": [
              "projects/demo-test/src/favicon.ico",
              "projects/demo-test/src/assets"
            ],
            "styles": [
              "projects/demo-test/src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}
